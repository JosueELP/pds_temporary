<%= render "partials/nav"%>
<div class="container">
  <div class="row">
    <div class="col">
      <h1>Consulta Sucursal</h1>
    </div>
  </div>
  <p id="notice"><%= notice %></p>

  <p class="tableInfo">
    <strong class="tableTitle">Nombre Sucursal:</strong>
    <%= @branch_office.branch_office_name %>
  </p>

  <p class="tableInfo">
    <strong class="tableTitle">Teléfono:</strong>
    <%= @branch_office.branch_office_phone %>
  </p>

  <p class="tableInfo">
    <strong class="tableTitle">Email:</strong>
    <%= @branch_office.branch_office_email %>
  </p>

  <%= link_to 'Editar', edit_branch_office_path(@branch_office), class:"tableButton" %> |
  <%= link_to 'Atrás', branch_offices_path, class:"tableButton" %>
</div>
