<p id="notice"><%= notice %></p>

<h1>Shippings</h1>

<table>
  <thead>
    <tr>
      <th>Expedition date</th>
      <th>Delivery date</th>
      <th>Type</th>
      <th>Authorization sign</th>
      <th>Delivery sign</th>
      <th>Package number</th>
      <th>Delivery cost</th>
      <th>Insurance cost</th>
      <th>Pick up cost</th>
      <th>Taxes</th>
      <th>Final cost</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shippings.each do |shipping| %>
      <tr>
        <td><%= shipping.expedition_date %></td>
        <td><%= shipping.delivery_date %></td>
        <td><%= shipping.type %></td>
        <td><%= shipping.authorization_sign %></td>
        <td><%= shipping.delivery_sign %></td>
        <td><%= shipping.package_number %></td>
        <td><%= shipping.delivery_cost %></td>
        <td><%= shipping.insurance_cost %></td>
        <td><%= shipping.pick_up_cost %></td>
        <td><%= shipping.taxes %></td>
        <td><%= shipping.final_cost %></td>
        <td><%= link_to 'Show', shipping %></td>
        <td><%= link_to 'Edit', edit_shipping_path(shipping) %></td>
        <td><%= link_to 'Destroy', shipping, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipping', new_shipping_path %>
